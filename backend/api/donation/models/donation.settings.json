{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "donations",
  "info": {
    "name": "donation"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "donation_to": {
      "type": "string"
    },
    "amount": {
      "type": "integer"
    },
    "message": {
      "type": "richtext"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "donations"
    }
  }
}
